{
  "name": "server-build",
  "version": "1.0.0",
  "description": "lamda",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "env-cmd -f .env.local nodemon ./index.js",
    "start": "node index.js",
    "clean": "rm -rf node_modules && yarn && yarn prepare",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "unit-test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "apicache": "^1.6.3",
    "babel-jest": "^28.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "express-session": "^1.17.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mal-scraper": "^2.11.4",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.1",
    "npm": "^8.17.0",
    "profanity-js": "^0.1.4",
    "promise-mysql": "^5.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "babel-loader": "^8.2.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "14.18.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "browsers": [
        "chrome",
        "firefox",
        "safari"
      ]
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    }
  },
  "main": "index.js",
  "keywords": []
}
