{
  "name": "server-build",
  "version": "1.0.0",
  "description": "lamda",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev:build-server": "NODE_ENV=development webpack --config webpack.server.js --mode=development -w",
    "dev:start": "env-cmd -f .env.local nodemon ./index.js",
    "start": "run-p dev:*",
    "prepare": "cd .. && husky install server/.husky",
    "clean": "rm -rf node_modules && npm install && npm run prepare",
    "lint": "eslint \"*.js\" \"**/*.js\" \"**/**/*.js\" \"**/**/**/*.js\" --fix",
    "unit-test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "aws-sdk": "^2.1192.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crawler": "^1.3.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "express-session": "^1.17.3",
    "huge-uploader-nodejs": "^1.2.1",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.21.11",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.7",
    "npm": "^8.17.0",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.4.2",
    "serverless-express": "^2.0.12",
    "stripe": "^8.222.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "babel-loader": "^8.2.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "14.18.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "browsers": [
        "chrome",
        "firefox",
        "safari"
      ]
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    }
  },
  "main": "index.js",
  "keywords": []
}
