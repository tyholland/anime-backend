{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["eslint-plugin-prettier"],
  "ignorePatterns": ["doc/**/*.js", "google-cloud-sdk/*"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "react/no-unescaped-entities": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "react/prop-types": 0,
    "indent": [
      "error",
      2,
      {
        "ignoredNodes": ["TemplateLiteral"]
      }
    ],
    "eol-last": ["error", "always"],
    "template-curly-spacing": "off"
  },
  "globals": {
    "require": "readonly",
    "process": "readonly",
    "module": "readonly",
    "res": "readonly",
    "jest": "readonly",
    "global": "readonly"
  }
}
